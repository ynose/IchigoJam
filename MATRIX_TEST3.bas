1 'MATRIX LED
10 CLS:CLV
20 POKE #700,#21,#81,#E1,#00
30 FOR M=#700 TO #702
40  R=I2CW(#70,M,1,#700,0)
50 NEXT

60  FOR J=#0 TO #FF
70   LC 12,5:?HEX$(C,2);
80   A=PEEK(J)*8
90   FOR C=0 TO 5
100   FOR D=0 TO 7
110    [D]=[D]<<1|PEEK(A+D)>>(7-C)
120   NEXT
130   R=I2CW(#70,#703,1,#800,16)
140   WAIT 2
150  NEXT
160 NEXT
170 END
